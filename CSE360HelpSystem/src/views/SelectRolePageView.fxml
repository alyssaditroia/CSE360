<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.SelectRolePageController">
    <Label text="Select Your Role" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
    
    <Button fx:id="adminButton" text="Admin" onAction="#redirectToAdminHomepage" visible="false"/>
    <Button fx:id="instructorButton" text="Instructor" onAction="#redirectToInstructorHomepage" visible="false"/>
    <Button fx:id="studentButton" text="Student" onAction="#redirectToStudentHomepage" visible="false"/>
    
    
    <Button fx:id="backButton" text="Back" onAction="#goBack" />
</VBox>
